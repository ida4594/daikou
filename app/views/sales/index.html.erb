<h1>売上一覧</h1>

<table>
  <thead>
    <tr>
      <th>コード</th>
      <th>売上</th>
    </tr>
  </thead>

  <tbody>
    <% @sales.each do |sale| %>
      <tr>
        <td><%= sale.code %></td>
        <td><%= sale.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= form_tag import_sales_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "インポート" %>
<% end %>
